<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_move_form_inherit_print_buttons" model="ir.ui.view">
    <field name="name">account.move.form.inherit.print.buttons</field>
    <field name="model">account.move</field>
    <field name="inherit_id" ref="account.view_move_form"/>
    <field name="arch" type="xml">
      <xpath expr="//header" position="inside">
        <button name="action_print_a4" type="object" string="Imprimir A4" class="btn-primary"/>
        <button name="action_print_ticket" type="object" string="Imprimir Ticket" class="btn-secondary"/>
      </xpath>
    </field>
  </record>
</odoo>
